# -*- mode: python -*-

Import("env")

env = env.Clone()

env.SConscript(
    dirs=[
        "cbp_histogram_ce",
    ],
    exports=[
        "env",
    ],
)

env.Library(
    target="sampling_estimator",
    source=[
        "sampling_estimator.cpp",
    ],
    LIBDEPS=["$BUILD_DIR/mongo/db/query_exec"],
)

env.CppUnitTest(
    target="sampling_estimator_test",
    source=[
        "sampling_estimator_test.cpp",
    ],
    LIBDEPS=[
        "$BUILD_DIR/mongo/db/service_context_test_fixture",
        "sampling_estimator",
    ],
)
