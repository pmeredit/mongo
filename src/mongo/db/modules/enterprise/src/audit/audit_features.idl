global:
    cpp_namespace: "mongo::audit"

feature_flags:
    featureFlagImprovedAuditing:
        description: Enable 'Improved Auditing' semantics
        cpp_varname: gFeatureFlagImprovedAuditing
        default: true
        version: 5.0

    featureFlagRuntimeAuditConfig:
        description: Enable 'Runtime Audit Configuration'
        cpp_varname: gFeatureFlagRuntimeAuditConfig
        default: false
